pipeline{
	agent any
	stages{
		stage ('One') {
	 		steps {
				echo 'hello karim'
	        	}
		}
	 	stage ('two') {
			steps {
	      		input 'do you want to proceed'
	        	}	
		}
		stage('Build') {
            		steps {
                	echo 'Building..'
            		}
        	}
        	stage('Test') { 
            		steps {
                	echo 'Testing..'
            		}
        	}
        	stage('Deploy') {
            		steps {
                	echo 'Deploying....'
            		}
		stage('fuck u') {
            		steps {
                	echo 'Get out of mind....'
            		}
        	}
	}
}
